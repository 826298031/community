+package com.nowcoder.community.entity;
+
+public class Page {
+    //当前页面
+    private int current=1;
+    //显示上线
+    private int limit=10;
+    //总页数
+    private int rows;
+    //查询路径（复用分页的链接）
+    private String path;
+
+    public int getCurrent() {
+        return current;
+    }
+
+    public void setCurrent(int current) {
+        if(current>=1){
+            this.current = current;
+        }
+    }
+
+    public int getLimit() {
+        return limit;
+    }
+
+    public void setLimit(int limit) {
+        if(limit>=1&&limit<=100){
+            this.limit = limit;
+        }
+    }
+
+    public int getRows() {
+        return rows;
+    }
+
+    public void setRows(int rows) {
+        if(rows>=0) {
+            this.rows = rows;
+        }
+    }
+
+    public String getPath() {
+        return path;
+    }
+
+    public void setPath(String path) {
+        this.path = path;
+    }
+    /**
+     * 获取当前页的起始行
+     *
+     * @return
+     */
+    public  int getoffset(){
+        return (current-1)*limit;
+    }
+    /**
+     * 获取总页数
+     *
+     * @return
+     */
+    public int getTotal(){
+        if(rows%limit==0){
+            return rows/limit;
+        }else{
+            return rows/limit +1;
+        }
+    }
+
+    /**
+     * 获取起始页码
+     * @return
+     */
+    public int getFrom(){
+        int from=current-2;
+        return from<1?1:from;
+    }
